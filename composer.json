{
  "name": "mimicreative/yii2-nicescroll",
  "description": "Nice Scroll Asset Bundle for Yii2",
  "license": "MIT License",
  "authors": [
    {
      "name": "MF Haqqi",
      "email": "me@haqqi.net",
      "homepage": "http://haqqi.net",
      "role": "Creator"
    }
  ],
  "keywords": ["yii2", "nicescroll", "jquery", "scroll"],
  "type": "yii2-extension",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "yiisoft/yii2": ">=2.0.0",
    "bower-asset/jquery.nicescroll": ">=3.6.8"
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  },
  "autoload": {
    "psr-4": {
      "mimicreative\\assets\\": "src/"
    }
  }
}
